{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decorator": true
    },
    "target": "es2022",
    "loose": true,
    "externalHelpers": false,
    "keepClassNames": true
  },
  "exclude": ["node_modules"]
}
